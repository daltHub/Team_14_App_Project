<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".meeting_setter">

    <TextView
        android:id="@+id/textViewmin"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="35dp"
        android:layout_marginTop="55dp"
        android:text="TextViewofmin"
        app:layout_constraintStart_toEndOf="@+id/Timesetter"
        app:layout_constraintTop_toBottomOf="@+id/calendarViewmeeting_setter"
        android:layout_marginLeft="35dp" />

    <CalendarView
        android:id="@+id/calendarViewmeeting_setter"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Timesetter"
        android:layout_width="160dp"
        android:layout_height="50dp"
        android:background="@drawable/button_selector_green"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="@string/add_a_time"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.704" />

    <TextView
        android:id="@+id/textViewhour"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="55dp"
        android:layout_marginEnd="35dp"
        android:text="TextViewofhour"
        app:layout_constraintEnd_toStartOf="@+id/Timesetter"
        app:layout_constraintTop_toBottomOf="@+id/calendarViewmeeting_setter"
        />

    <Button
        android:id="@+id/buttonconfirmsetting"
        android:layout_width="160dp"
        android:layout_height="50dp"
        android:background="@drawable/button_selector_green"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="@string/finish"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.955" />

    <Button
        android:id="@+id/button7"
        android:layout_width="160dp"
        android:layout_height="50dp"
        android:background="@drawable/button_selector_green"
        android:fontFamily="sans-serif-condensed-medium"
        android:text="@string/set_more_dates"
        android:textAppearance="@style/TextAppearance.AppCompat.Large"
        android:textColor="#000000"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.829" />

</androidx.constraintlayout.widget.ConstraintLayout>